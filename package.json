{
  "name": "izn-boilerplate",
  "description": "Basic web development boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "M Arif Mahthunisnaini",
    "url": "http://iznyn.github.io/"
  },
  "repository": "iznyn/izn-boilerplate",
  "scripts": {
    "start": "npm run serve",
    "dev": "grunt watch",
    "build": "grunt build",
    "serve": "grunt serve",
    "docs-dev": "cross-env process.env.section=docs grunt watch",
    "docs-serve": "cross-env process.env.section=docs grunt serve",
    "docs-build": "cross-env process.env.section=docs grunt build",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint resources/sass/**/*.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern public",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern public --fix",
    "lint:js": "npm run lint:eslint -- . "
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@lodder/grunt-postcss": "^3.0.0",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "grunt": "^1.3.0",
    "grunt-babel": "^8.0.0",
    "grunt-browserify": "^6.0.0",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-connect": "^3.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-pug": "^3.0.0",
    "grunt-contrib-uglify": "^5.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-sass": "^3.1.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.8",
    "postcss-focus": "^5.0.1",
    "postcss-fontpath": "^1.0.0",
    "postcss-scss": "^3.0.5",
    "postcss-short": "^5.0.0",
    "prettier": "^2.2.1",
    "pug": "^3.0.2",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "tsify": "^5.0.2",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "bootstrap-sass": "^3.4.1",
    "compass-mixins": "^0.12.10",
    "imagesloaded": "^4.1.4",
    "jquery": "^3.6.0",
    "spinkit": "^2.0.1"
  }
}
